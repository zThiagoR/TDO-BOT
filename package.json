{
  "name": "discord-bot-template-ts",
  "version": "1.0.0",
  "description": "Template Discord Bot TypeScript",
  "main": "./build/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --poll --transpile-only --ignore-watch ./node_modules ./src/app.ts",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "start": "node ."
  },
  "keywords": [
    "discord",
    "bot",
    "template",
    "typescript"
  ],
  "author": "Tiago Ryan",
  "license": "MIT",
  "dependencies": {
    "ark.db": "^2.7.1",
    "colors": "^1.4.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "moment": "^2.29.3",
    "moment-duration-format": "^2.3.2",
    "ms": "^2.1.3",
    "rimraf": "^3.0.2",
    "simpl.db": "^2.10.1"
  },
  "devDependencies": {
    "@discordjs/builders": "^0.12.0",
    "@types/moment-duration-format": "^2.2.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.23",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "16.x"
  }
}
